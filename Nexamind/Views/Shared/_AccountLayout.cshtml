    <script src="https://apis.google.com/js/platform.js" async defer></script>



</head>
<body>

    <div class="container-fluid">
        <partial name="_CookieConsentPartial" />

        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

